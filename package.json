{
  "name": "@fkui/user-lib-monorepo",
  "version": "1.2.2",
  "private": true,
  "description": "fkui custom user monorepo",
  "homepage": "https://forsakringskassan.github.io/designsystem-user-lib/",
  "bugs": "https://github.com/Forsakringskassan/designsystem-user-lib/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Forsakringskassan/designsystem-user-lib.git"
  },
  "license": "MIT",
  "author": "Försäkringskassan",
  "main": "index.js",
  "files": [],
  "workspaces": [
    "docs",
    "packages/*"
  ],
  "scripts": {
    "build": "lerna run build",
    "build:docs": "node generate-docs.mjs && lerna run build:docs && npm run build:public",
    "build:public": "html-validate --formatter stylish,codeframe --config .htmlvalidate-public.js public",
    "preclean": "rimraf -g .eslintcache *.tgz public temp test-results",
    "clean": "npm run --workspaces --if-present clean",
    "design": "npm run --workspace packages/design --",
    "eslint": "eslint --cache .",
    "eslint:fix": "eslint --fix .",
    "html-validate": "html-validate --formatter stylish,codeframe --ext md,html,vue docs packages/*/src",
    "lerna": "lerna",
    "link-all": "lerna exec npm link",
    "lint": "run-s prettier:check eslint stylelint html-validate",
    "lint:fix": "run-s prettier:write eslint:fix stylelint:fix",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "start": "node docs/serve.mjs public",
    "stylelint": "stylelint \"packages/*/src/**/*.scss\"",
    "stylelint:fix": "npm run stylelint -- --fix",
    "pretest": "npm run lint",
    "test": "lerna run test",
    "vue": "npm run --workspace packages/vue --"
  },
  "commitlint": {
    "extends": "@forsakringskassan/commitlint-config/no-jira"
  },
  "prettier": "@forsakringskassan/prettier-config",
  "release": {
    "extends": "@forsakringskassan/semantic-release-monorepo-config"
  },
  "stylelint": {
    "extends": [
      "@forsakringskassan/stylelint-config",
      "@fkui/design/stylelint/recommended"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.29.0",
    "@babel/preset-env": "7.29.0",
    "@fkui/date": "6.36.0",
    "@fkui/vue": "6.36.0",
    "@forsakringskassan/commitlint-config": "3.3.3",
    "@forsakringskassan/docs-generator": "2.35.7",
    "@forsakringskassan/docs-live-example": "2.4.1",
    "@forsakringskassan/eslint-config": "13.3.2",
    "@forsakringskassan/eslint-config-cli": "13.3.2",
    "@forsakringskassan/eslint-config-jest": "13.3.4",
    "@forsakringskassan/eslint-config-typescript": "13.3.0",
    "@forsakringskassan/eslint-config-typescript-typeinfo": "13.3.0",
    "@forsakringskassan/eslint-config-vue": "13.3.4",
    "@forsakringskassan/jest-config": "29.9.0",
    "@forsakringskassan/jest-config-vue": "29.9.0",
    "@forsakringskassan/prettier-config": "3.3.4",
    "@forsakringskassan/stylelint-config": "3.3.2",
    "@forsakringskassan/vite-lib-config": "4.3.4",
    "@html-validate/release-scripts": "7.1.0",
    "@types/estree": "1.0.8",
    "@types/glob": "9.0.0",
    "@types/jest": "29.5.14",
    "@types/lodash": "4.17.23",
    "@types/node": "20.19.33",
    "@vue/server-renderer": "3.5.28",
    "autoprefixer": "10.4.24",
    "cli-table": "0.3.11",
    "core-js": "3.48.0",
    "esbuild": "0.27.3",
    "flush-promises": "1.0.2",
    "fs-extra": "11.3.3",
    "glob": "13.0.3",
    "highlight.js": "11.11.1",
    "html-validate": "10.7.0",
    "html-validate-markdown": "5.1.0",
    "html-validate-vue": "7.4.2",
    "is-ci": "4.1.0",
    "lerna": "9.0.4",
    "mocha-junit-reporter": "2.2.1",
    "mocha-multi-reporters": "1.5.1",
    "npm-pkg-lint": "4.3.4",
    "npm-run-all2": "8.0.4",
    "postcss": "8.5.6",
    "rimraf": "6.1.2",
    "sass": "1.97.3",
    "typedoc": "0.28.16",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "vue": "3.5.28",
    "vue-tsc": "3.2.4"
  },
  "engines": {
    "node": ">= 20",
    "npm": ">= 7"
  }
}
